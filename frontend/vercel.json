{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://cpf-wl-server.vercel.app/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}
